        "doctrine/inflector": "^1.4|^2.0",
        "illuminate/database": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.15|^8.4|^9.0",
        "illuminate/events": "^6.0|^7.0|^8.0",
        "fakerphp/faker": "^1.16"
    }
}
