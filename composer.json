{
    "name": "anik/dataset",
    "description": "Dataset is a php package for importing & exporting data within CSV & Database",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Syed Sirajul Islam Anik",
            "email": "sirajul.islam.anik@gmail.com"
        }
